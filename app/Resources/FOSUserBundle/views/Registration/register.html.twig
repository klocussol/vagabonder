{% extends "VagabonderWebBundle::layout.html.twig" %}

{% block title %}Vagabonder - Registration{% endblock title %}

{% block content %}

{% block fos_user_content %}

<div class="row">
    <div class="large-12 columns">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" type="vagabonder_user_registration">
            {{ form_widget(form) }}

            <div>
                <input type="submit" value="{{ 'Submit'|trans }}" />
            </div>
        </form>
    </div>
</div>

{% endblock fos_user_content %}

{% endblock content %}
